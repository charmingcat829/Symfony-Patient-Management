    <div class="col-md-6">  
	    <div class="box hide">
		    <header>
		        <h4><span class="glyphicon glyphicon-file"></span> Situation epidemiologique</h4>
		    </header>
		    <table class="table table-striped table-bordered">
		    <thead>
			    <tr>
			    	<th>Cas</th>
			    	<th>nbre</th>
			    </tr>
		    </thead>
		        {% for item in stats.consultations %}
		        <tr>
		            <td>{{ item.label }}</td>
		            <td>{{ item.data }}</td>
		        </tr>
		        {% endfor %}
		    </table>
	    </div>
	    <div class="box">
		    <header>
		        <h4><span class="glyphicon glyphicon-file"></span> Couverture sociale</h4>
