    <div class="col-md-8">  
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Situation epidemiologique</h4>
	    </header>
	    <table class="table table-striped table-bordered">
	    <thead>
		    <tr>
		    	<th>Cas</th>
		    	<th>nbre</th>
		    </tr>
	    </thead>
	        {% for item in stats.consultations %}
	        <tr>
	            <td>{{ item.label }}</td>
	            <td>{{ item.data }}</td>
	        </tr>
	        {% endfor %}
	    </table>
    </div>
    </div>
    <div class="col-md-4">
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Médicament délivré par le centre</h4>
	    </header>
	    <table class="table table-striped table-bordered">
	    <thead>
		    <tr>
		    	<th>Medicament</th>
		    	<th>nbre</th>
		    </tr>
	    </thead>
	        {% for item in stats.meds %}
	        <tr>
	            <td>{{ item.label }}</td>
	            <td>{{ item.data }}</td>
	        </tr>
	        {% endfor %}
	    </table>
    </div>
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Nombre de médicament dans le stock {{ stats.stock }}</h4>
	    </header>
	</div>
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Nombre de consultation {{ stats.general_consultations_count }}</h4>
	    </header>
	</div>
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Nombre d'etudiants orientés {{ stats.oriented_persons }}</h4>
	    </header>
	</div>
    <div class="box">
	    <header>
	        <h4><span class="glyphicon glyphicon-file"></span> Nombre de consultations specialisée {{ stats.special_consultations_count }}</h4>
	    </header>
	</div>
    </div>
    <div class="clearfix"></div>